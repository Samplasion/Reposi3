<package>
	<id>com.rpetrich.libactivator</id>
	<name>Activator</name>
	<version>1.9.12</version>
	<compatibility>
		<firmware>
			<maxiOS>10.3.3</maxiOS>
			<otherVersions>unsupported</otherVersions>
		</firmware>
	</compatibility>
	<dependencies>
		<package>mobilesubstrate >= 0.9.5101</package>
		<package>preferenceloader >= 2.2.2</package>
		<package>FlipSwitch >= 1.0.15</package>
		<package>RocketBootstrap >= 1.0.5 or iOS Firmware < 7.0</package>
	</dependencies>
	<descriptionlist>
		<description>Centralized gestures, button and shortcut management for iOS</description>
	</descriptionlist>
