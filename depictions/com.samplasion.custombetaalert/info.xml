<package>
	<id>com.samplasion.custombetaalert</id>
	<name>CustomBetaAlert</name>
	<version>1.0.0</version>
	<license>Open Source</license>
	<size>18 KB</size>
	<compatibility>
		<firmware>
			<miniOS>11.0</miniOS>
		</firmware>
	</compatibility>
	<dependencies>
		<package>MobileSubstrate</package>
	</dependencies>
	<descriptionlist>
		<description> <![CDATA[
			<i>Supports iOS 12</i><br>
			Recreate the annoying beta update message, but now with custom text!<br>
			Supports placeholders: "%iOS" for the current iOS version; "%batt" for the current battery percentage and "%model" for the current device model.<br>
			<br>
			<i>Configure from settings</i>
			]]></description>
	</descriptionlist>
	<screenshots>
		<screenshot>
			<description>This is a description for screenshot 1. It's Cydia!</description>
			<video>preview.mov</video>
		</screenshot>
	</screenshots>
	<changelog>
		<change>First release</change>
	</changelog>
	<links>
		<link>
			<name>/r/jailbreak</name>
			<url>https://www.reddit.com/r/jailbreak</url>
			<iconclass>fa fa-reddit</iconclass>
		</link>
		<link>
			<name>Source Code</name>
			<url>https://github.com/Samplasion/custombetaalert</url>
			<iconclass>fa fa-github</iconclass>
		</link>
	</links>
</package>
